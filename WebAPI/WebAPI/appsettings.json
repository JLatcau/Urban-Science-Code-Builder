{
  "ConnectionStrings": {
    "UserRequestsAppCon": "Host=localhost;Database=postgres;Port=5432;User Id=postgres;Password=postgres",
    "GeneratedCodeAppCon": "Host=localhost;Database=postgres;Port=5432;User Id=postgres;Password=postgres"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "WebAPILoggerProvider": {
      "Options": {
        "FolderPath": "{localPath}\\WebAPI\\WebAPI\\Logging\\Logs",
        "FilePath": "error_log.log"
      },
      "LogLevel": {
        "default": "Error",
        "Microsoft": "Error",
        "WebAPI.Web.Controllers": "Error",
        "Microsoft.AspNetCore": "Error"

      }
    }
  },
 
  "runtimeOptions": {
    "configProperties": {
      "Switch.System.Data.AllowArbitraryDataSetTypeInstantiation": true
    }
  },
  "AllowedHosts": "*"
}
